{
  "name": "grafana_prediction_plugin",
  "productName": "VRAM Software - Grafana prediction",
  "version": "0.0.1",
  "author": "vram-software",
  "description": "Plugin Grafana Gruppo VRAM",
  "private": true,
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "ci-test": "grafana-toolkit plugin:test --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch"
  },
  "license": "Apache-2.0",
  "jest": {
    "moduleNameMapper": {
      "grafana/app/plugins/sdk": "<rootDir>/node_modules/grafana-sdk-mocks/app/plugins/sdk.ts"
    }
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "plotly.js": "^1.41",
    "scriptjs": "^2.5.9"
  },
  "devDependencies": {
    "@grafana/data": "next",
    "@grafana/toolkit": "next",
    "@grafana/ui": "next",
    "@types/grafana": "github:CorpGlory/types-grafana.git",
    "grafana-sdk-mocks": "github:grafana/grafana-sdk-mocks",
    "coveralls": "^3.0.9"
  }
}
